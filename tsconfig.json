{
  "compilerOptions": {
    "declaration": true,
    "declarationMap": true,
    "downlevelIteration": true,
    "esModuleInterop": true,
    "moduleResolution": "node",
    "noFallthroughCasesInSwitch": true,
    // "noUnusedLocals": true,
    // "noUnusedParameters": true,
    "strict": true,
    "sourceMap": true,
    "target": "es2015",
    "lib": ["es2015", "DOM", "DOM.Iterable"]
  }
}
